{
  "expo": {
    "name": "foodma",
    "slug": "foodma",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "foodma",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "FoodMa needs access to your location to show nearby restaurants and deliver food to your address.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "FoodMa needs access to your location to show nearby restaurants and deliver food to your address."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "FoodMa needs access to your location to show nearby restaurants and deliver food to your address.",
          "locationWhenInUsePermission": "FoodMa needs access to your location to show nearby restaurants and deliver food to your address."
        }
      ],
      "expo-asset",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5464198d-81aa-4962-8995-da90c3564d51"
      }
    },
    "owner": "abdullahabubakar461"
  }
}
